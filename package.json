{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "element-theme-chalk": "^2.8.0",
    "element-ui": "^2.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.5.21",
    "vuex": "^3.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        2,
        2
      ],
      "no-console": 0,
      "no-new": 0,
      "promise/param-names": 0,
      "valid-typeof": 0,
      "comma-dangle": [
        0,
        "never"
      ],
      "semi": [
        0,
        "always"
      ],
      "eqeqeq": 0,
      "no-trailing-spaces": 0,
      "spaced-comment": 0,
      "padded-blocks": 0,
      "generator-star-spacing": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
